*/
area|z15-[building], relation[building] {
    z-index: 9;
    fill-color: #dde0e4;
    color: #8b929b;
    width: 0.5;
    /* extrude: eval('zmetric(tag("height"))'); // not supported */
}

relation[building] way {
    color: #8b929b;
    width: 0.5;
}

/************************************************

           █  █
              █
  ██  ███  █  █   ███
