line[railway=rail] {
    z-index: 7;
    color: #dddddd;
    width: 1.0;
    casing-width: 0.5;
    casing-color: #bbbbbb;
    dashes: 1,1;
}

way[railway=rail][usage=main] {
    casing-color: #666666;
    dashes: 4,4;
}

way|z17[railway=rail] { width: 2.0; dashes: 1,2; }
way|z18-[railway=rail] { width: 4.0; dashes: 1,4; }

way[railway=rail][service] { width: 0.2; }
way|z17[railway=rail][service] { width: 0.5; }
way|z18-[railway=rail][service] { width: 0.7; }

/* Tram */

way|z8-[railway=tram] {
    z-index: 8;
    color: #330000;
    width: 1.0;
    casing-color: #330000;
    casing-width: 1.0;
    casing-dashes: 1,7;
}

way|z17[railway=tram] { width: 2.0; }
way|z18-[railway=tram] { width: 4.0; }

/* Tram routes are bolder than service tram rails */

relation[type=route] line|z8-[railway=tram] { width: 3; casing-width: 2.0; }
relation[type=route] line|z12-[railway=tram] { width: 4; }
relation[type=route] line|z16-[railway=tram] { width: 6; }
relation[type=route] line|z18-[railway=tram] { width: 8; }

node[railway=tram_stop] {
    z-index: 9;
    width: 6;
    color: #ff0000;
    casing-width: 1;
    casing-color: #330000;
}

node|z12-[railway=tram_stop] { width: 7; }
node|z16-[railway=tram_stop] { width: 10; }

