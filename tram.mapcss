             █                        █
          ███                        █

*/

/* important highways */
way[highway] {
    z-index: 6;
    color: #ffffff;
    linecap: round;
    casing-linecap: round;
    casing-width: 0.5;
    casing-color: #ddaa77;
    linejoin: bevel;
    casing-linejoin: bevel;
    width: 1.5;
    
    text: name;
    text-color: #33000080;
    font-family: "Open Sans";
    font-size: 9;
    text-position: line;
}

way[highway=trunk],
way[highway=trunk_link], way[highway=motorway], way[highway=motorway_link] {
    casing-width: 0.95;
    color: #ffcddc;
    width: 2;
    font-size: 16;
}


way[highway=primary], way[highway=primary_link], way[highway=trunk],
way[highway=trunk_link], way[highway=motorway], way[highway=motorway_link] {
    casing-width: 0.8;
    color: #ffdccd;
    font-size: 14;
}

way[highway=secondary], way[highway=secondary_link] {
    casing-width: 0.65;
    color: #fff2cd;
    font-size: 12;
}

way|z15[highway] { width : 3.0; }
way|z16[highway] { width : 5.0; }
way|z17[highway] { width : 8.0; }
way|z18-[highway] { width : 10.0; }

/* less important highways */
way|z1-15[highway=service],
way[highway=track],
way[highway=path],
way[highway=footway],
way[highway=cycleway],
way[highway=bridleway]{
    z-index: 5;
    width: 0.5;
    color: #bb9966;
    casing-width: 0;
    casing-dashes: 0;
    image: none;
}

way[highway=steps] {
    z-index: 5;
    width: 6;
    dashes: 2,2;
    color: #999988;
    casing-width: 0;
    casing-dashes: 0;
    image: none;
    linecap: none;
    linejoin: bevel;
}

way|z17[highway=steps] {
    width: 3;
    dashes: 1,2;
}

way|z16[highway=steps] {
    width: 3;
    color: #666666;
    dashes: 0;
}

way|z1-15[highway=steps] {
    width: 2;
    color: #888888;
    dashes: 0;
}


way|z16-[highway=service],
way|z16-[highway=footway],
way|z16-[highway=cycleway],
way|z16-[highway=bridleway],
way|z16-[highway=track] {
    width: 1.0;
}

way[highway=footway],
way[highway=cycleway],
way[highway=bridleway]{
    dashes: 4,2;
}

way[highway=path] {
    dashes: 3,3;
}

way[highway=track] {
    dashes: 4,4;
}

way[bridge][highway] {
    casing-width: 2.0;
    casing-linecap: none;
    casing-dashes: 0;
}

